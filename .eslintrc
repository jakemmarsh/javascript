{
  "rules": {
    "curly": [2, "all"],
    "eqeqeq": 2,
    "max-depth": [2, 5],
    "max-params": [2, 6],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-loop-func": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "strict": 2,
    "wrap-iife": [2, "inside"]
  }
}